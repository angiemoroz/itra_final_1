<!DOCTYPE html>
<html>
  <head>
    <title>ItraFinal1</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'jquery' %>
    
<link href="https://cdnjs.cloudflare.com/ajax/libs/select2/4.0.3/css/select2.min.css" rel="stylesheet" />
<script src="https://cdnjs.cloudflare.com/ajax/libs/select2/4.0.3/js/select2.min.js"></script>

  </head>

  <body>
          <% if user_signed_in? %>
  	      <tr><%= link_to 'Sign out', destroy_user_session_path, :method => :delete, class: "btn btn-default sign-out" %></tr>
          <tr><%= link_to 'Profile', current_user , class: "btn btn-default prof" %></tr>
          <% end %>
          <tr><%= link_to 'Home', user_instructions_path, class: "btn btn-default home" %></tr>
    <%= yield %>
    
 </body>
</html>
